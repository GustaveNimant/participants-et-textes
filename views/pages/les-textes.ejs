<!DOCTYPE html>
<html>
    <head>
        <title><%= title_tag %></title>
    </head>

    <body>
        <h1><%= title_page %></h1>
	<ul>
	    <% textes.forEach(function (un_texte) {%>
		<li><%= un_texte.titreTexte %> par <%= un_texte.auteurTexte %></li>

		<a href="/texte-affichage-par-id/<%= un_texte._id %>">
		    Afficher
		</a>
		<br>
		<a href="/texte-suppression-par-id/<%= un_texte._id %>">
		    Supprimer
		</a>

	    <% }); %>
	</ul>
    </body>
</html>
