<!DOCTYPE html>
<html>
    <head>
        <title><%= pseudo_tag %></title>
    </head>

    <body>
        <h1><%= pseudo_page %></h1>
	<ul>
	    <% participants.forEach(function (un_participant) {%>
		<li>
		    Pseudo 
		    "<%= un_participant.pseudoParticipant %>"
		    Email
		    "<%= un_participant.emailParticipant %>"
		</li>
		
		<a href="/participant-affichage-par-id/<%= un_participant._id %>">
		    Afficher
		</a>
		<br>
		<a href="/participant-suppression-par-id/<%= un_participant._id %>">
		    Supprimer
		</a>

	    <% }); %>
	</ul>
    </body>
</html>
